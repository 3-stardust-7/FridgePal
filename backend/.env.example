# FridgePal Backend Environment Variables
# Copy this file to .env and adjust values as needed

# Server settings
HOST=0.0.0.0
PORT=8000
DEBUG=false

# Model settings
YOLO_MODEL_PATH=yolov8n.pt
DETECTION_CONFIDENCE=0.5

# OCR settings (optional - set if Tesseract is not in PATH)
# TESSERACT_PATH=C:\Program Files\Tesseract-OCR\tesseract.exe
OCR_CONFIDENCE=0.6

# Image processing
MAX_IMAGE_SIZE=800
JPEG_QUALITY=90

# Logging
LOG_LEVEL=INFO
